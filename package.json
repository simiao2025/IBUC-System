  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:check": "npm run type-check && npm run build",
    "lint": "eslint .",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",

    "deploy": "npm run build:check && echo 'Build completed! Upload the dist folder to Netlify'",
    "deploy:netlify": "npm run build && npx netlify deploy --prod --dir=dist",
    "deploy:check": "npm run type-check && npm run build && echo 'âœ… Build successful - ready for deployment!'",
    "test:build": "npm run build && npx serve dist -s"
  },
  "dependencies": {
